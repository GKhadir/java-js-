class switch1 
{
	public static void main(String[] args) 
	{
		switch(4)
		{
			case 1:
			{
				System.out.println("Hello World!");
			}
			break;
			case 2:
			{
				System.out.println("Hello ");
			}
			break;
			case 3:
			{
				System.out.println(" World!");
			}
			break;
			case 4:
			{
				System.out.println("bye");
			}
			break;
			default :
			{
				System.out.println("khadir");
			}
		}
	}
}
