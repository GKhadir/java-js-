class circlemethod
{
	static void circle(int r)
	{
		
		double pi=3.142;
		
		
		double result=pi*r*r;
		System.out.println("area of circle:"+result);
	}
	public static void main(String[] args) 
	{
		System.out.println("*******main starts******");
		circle(5);
		System.out.println("*******main ends******");
	}
}







